                <invoice_url_update_sms_template></invoice_url_update_sms_template>
            </sms_template>
            <sms_security>
                <enable>1</enable>
                <sms_code_lifetime>300</sms_code_lifetime>
                <number_interval>60</number_interval>
                <number_limit>30</number_limit>
                <arrival_notice_captcha_limit>10</arrival_notice_captcha_limit>
                <ip_limit>200</ip_limit>
            </sms_security>
        </sms_setting>
    </default>
</config>
