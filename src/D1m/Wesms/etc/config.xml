                <arrival_notice_request_enable>0</arrival_notice_request_enable>
                <arrival_notice_request_sms_template></arrival_notice_request_sms_template>
                <news_enable>0</news_enable>
                <news_sms_template></news_sms_template>
                <registration_welcome_enable>0</registration_welcome_enable>
                <registration_welcome_template></registration_welcome_template>
                <complete_enable>0</complete_enable>
                <complete_sms_template></complete_sms_template>
                <invoice_url_enable>0</invoice_url_enable>
