<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                        <resource id="D1m_Wesms::wesms_sms_log" title="WE SMS Log" sortOrder="10"/>
                        <resource id="D1m_Wesms::wesms_sms_tag" title="WE SMS Log" sortOrder="10"/>
                        <resource id="D1m_Wesms::wesms_sms_template" title="WE SMS Template" sortOrder="20"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="D1m_Wesms::sms_setting" title="SMS Setting"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
